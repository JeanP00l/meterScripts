	function incrementSeal(raw) {
		const prefix = raw.replace(/\d+$/, '');
		const numMatch = raw.match(/(\d+)$/);
		if (!numMatch) throw new Error('Ğ§Ğ¸ÑĞ»Ğ¾Ğ²Ğ°Ñ Ñ‡Ğ°ÑÑ‚ÑŒ Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½Ğ° Ğ² seal');
		const numStr = numMatch[1];
		const inc = (parseInt(numStr, 10) + 1)
			.toString()
			.padStart(numStr.length, '0');
		return prefix + inc;
	}

	async function getStableInputValue(input, attempts = 5, delay = 50) {
		let lastVal = input.value;
		for (let i = 0; i < attempts; i++) {
			await wait(delay);
			if (input.value === lastVal) return lastVal;
			lastVal = input.value;
		}
		return lastVal;
	}

	// Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‹ Ñ React checkbox
	async function setReactCheckboxValue(selector, checked, maxRetries = 3, silent = false) {
		const checkbox = await waitForSelector(selector);
		if (!checkbox || checkbox.type !== 'checkbox') {
			return false;
		}
		
		// Ğ¤Ğ»Ğ°Ğ³ Ğ´Ğ»Ñ Ğ²Ñ€ĞµĞ¼ĞµĞ½Ğ½Ğ¾Ğ¹ Ğ±Ğ»Ğ¾ĞºĞ¸Ñ€Ğ¾Ğ²ĞºĞ¸ workflow Ğ¿Ñ€Ğ¸ silent Ñ€ĞµĞ¶Ğ¸Ğ¼Ğµ
		let workflowBlocked = false;
		if (silent) {
			window._blockWorkflow = true;
			workflowBlocked = true;
		}
		
		try {
			for (let attempt = 1; attempt <= maxRetries; attempt++) {
				const lastChecked = checkbox.checked;
				
				// Ğ˜ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞµĞ¼ Ğ½Ğ°Ñ‚Ğ¸Ğ²Ğ½Ñ‹Ğ¹ setter Ğ´Ğ»Ñ Ğ¿Ñ€Ğ°Ğ²Ğ¸Ğ»ÑŒĞ½Ğ¾Ğ¹ Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‹ Ñ React
				const prototype = window.HTMLInputElement.prototype;
				const nativeCheckedSetter = Object.getOwnPropertyDescriptor(prototype, 'checked')?.set;
				
				if (nativeCheckedSetter) {
					nativeCheckedSetter.call(checkbox, checked);
				} else {
					checkbox.checked = checked;
				}
				
				// Ğ Ğ°Ğ±Ğ¾Ñ‚Ğ°ĞµĞ¼ Ñ React Ñ‚Ñ€ĞµĞºĞµÑ€Ğ¾Ğ¼ (Ğ´Ğ»Ñ checkbox Ñ‚Ğ¾Ğ¶Ğµ Ğ¸ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞµÑ‚ÑÑ _valueTracker)
				const tracker = checkbox._valueTracker;
				if (tracker) {
					try {
						// Ğ”Ğ»Ñ checkbox Ñ‚Ñ€ĞµĞºĞµÑ€ Ñ€Ğ°Ğ±Ğ¾Ñ‚Ğ°ĞµÑ‚ Ñ Ğ±ÑƒĞ»ĞµĞ²Ñ‹Ğ¼ Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸ĞµĞ¼
						tracker.setValue(lastChecked);
					} catch (e) {
						// Ğ˜Ğ³Ğ½Ğ¾Ñ€Ğ¸Ñ€ÑƒĞµĞ¼ Ğ¾ÑˆĞ¸Ğ±ĞºĞ¸ Ñ‚Ñ€ĞµĞºĞµÑ€Ğ°
					}
				}
				
				// Ğ”Ğ¸ÑĞ¿Ğ°Ñ‚Ñ‡Ğ¸Ğ¼ ÑĞ¾Ğ±Ñ‹Ñ‚Ğ¸Ñ Ñ bubbles: true Ğ´Ğ»Ñ React
				const changeEvent = new Event('change', { bubbles: true, cancelable: true });
				const inputEvent = new Event('input', { bubbles: true, cancelable: true });
				const clickEvent = new Event('click', { bubbles: true, cancelable: true });
				
				// Ğ”Ğ¾Ğ±Ğ°Ğ²Ğ»ÑĞµĞ¼ Ñ„Ğ»Ğ°Ğ³ Ğ´Ğ»Ñ Ğ¸Ğ´ĞµĞ½Ñ‚Ğ¸Ñ„Ğ¸ĞºĞ°Ñ†Ğ¸Ğ¸ Ğ¿Ñ€Ğ¾Ğ³Ñ€Ğ°Ğ¼Ğ¼Ğ½Ñ‹Ñ… ÑĞ¾Ğ±Ñ‹Ñ‚Ğ¸Ğ¹
				if (silent) {
					changeEvent._programmatic = true;
					inputEvent._programmatic = true;
					clickEvent._programmatic = true;
				}
				
				checkbox.dispatchEvent(clickEvent);
				checkbox.dispatchEvent(inputEvent);
				checkbox.dispatchEvent(changeEvent);
				
				await wait(150);
				
				// ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼, Ñ‡Ñ‚Ğ¾ Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ğµ ÑƒÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ¸Ğ»Ğ¾ÑÑŒ
				if (checkbox.checked === checked) {
					return true;
				}
				
				// Ğ•ÑĞ»Ğ¸ Ğ½Ğµ ÑƒÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ¸Ğ»Ğ¾ÑÑŒ Ğ¸ ÑÑ‚Ğ¾ Ğ½Ğµ Ğ¿Ğ¾ÑĞ»ĞµĞ´Ğ½ÑÑ Ğ¿Ğ¾Ğ¿Ñ‹Ñ‚ĞºĞ° - Ğ¶Ğ´ĞµĞ¼ Ğ¸ Ğ¿Ñ€Ğ¾Ğ±ÑƒĞµĞ¼ ÑĞ½Ğ¾Ğ²Ğ°
				if (attempt < maxRetries) {
					await wait(200);
				}
			}
			
			// Ğ•ÑĞ»Ğ¸ Ğ¿Ğ¾ÑĞ»Ğµ Ğ²ÑĞµÑ… Ğ¿Ğ¾Ğ¿Ñ‹Ñ‚Ğ¾Ğº Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ğµ Ğ½Ğµ ÑƒÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ¸Ğ»Ğ¾ÑÑŒ
			if (checkbox.checked !== checked) {
				return false;
			}
			
			return true;
		} finally {
			// Ğ¡Ğ½Ğ¸Ğ¼Ğ°ĞµĞ¼ Ğ±Ğ»Ğ¾ĞºĞ¸Ñ€Ğ¾Ğ²ĞºÑƒ workflow Ğ¿Ğ¾ÑĞ»Ğµ Ğ½ĞµĞ±Ğ¾Ğ»ÑŒÑˆĞ¾Ğ¹ Ğ·Ğ°Ğ´ĞµÑ€Ğ¶ĞºĞ¸
			if (workflowBlocked) {
				setTimeout(() => {
					window._blockWorkflow = false;
				}, 500);
			}
		}
	}

	// Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ¸ÑĞºĞ° Ğ¿Ğ¾Ğ»Ñ Ñ€ĞµĞ·ÑƒĞ»ÑŒÑ‚Ğ°Ñ‚Ğ° Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸ Ğ¿Ğ¾ Ñ‚ĞµĞºÑÑ‚Ñƒ
