            return;
          }
          
          // ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ Ğ¿Ğ°Ñ‚Ñ‚ĞµÑ€Ğ½Ñ‹ ĞœĞµÑ€ĞºÑƒÑ€Ğ¸Ğ¹
          let counterType = null;
          
          // ĞĞ¾Ñ€Ğ¼Ğ°Ğ»Ğ¸Ğ·ÑƒĞµĞ¼ Ñ„Ñ€Ğ°Ğ·Ñƒ: Ğ·Ğ°Ğ¼ĞµĞ½ÑĞµĞ¼ "Ğ¸" Ğ¸ Ğ·Ğ°Ğ¿ÑÑ‚ÑƒÑ Ğ½Ğ° Ñ‚Ğ¾Ñ‡ĞºÑƒ, ÑƒĞ±Ğ¸Ñ€Ğ°ĞµĞ¼ Ğ»Ğ¸ÑˆĞ½Ğ¸Ğµ Ğ¿Ñ€Ğ¾Ğ±ĞµĞ»Ñ‹
          let normalizedForMercury = counterPhrase.toLowerCase()
            .replace(/\s+/g, ' ')
            .replace(/\s*Ğ¸\s*/g, '.')
            .replace(/,/g, '.')  // Ğ—Ğ°Ğ¿ÑÑ‚Ğ°Ñ Ñ‚Ğ¾Ğ¶Ğµ Ğ¼Ğ¾Ğ¶ĞµÑ‚ Ğ±Ñ‹Ñ‚ÑŒ Ñ€Ğ°Ğ·Ğ´ĞµĞ»Ğ¸Ñ‚ĞµĞ»ĞµĞ¼
