	async function setReactInputValue(selector, value, maxRetries = 3, silent = false) {
		const input = await waitForSelector(selector);
		
		// Ğ¤Ğ»Ğ°Ğ³ Ğ´Ğ»Ñ Ğ²Ñ€ĞµĞ¼ĞµĞ½Ğ½Ğ¾Ğ¹ Ğ±Ğ»Ğ¾ĞºĞ¸Ñ€Ğ¾Ğ²ĞºĞ¸ workflow Ğ¿Ñ€Ğ¸ silent Ñ€ĞµĞ¶Ğ¸Ğ¼Ğµ
		let workflowBlocked = false;
		if (silent) {
			window._blockWorkflow = true;
			workflowBlocked = true;
		}
		
		try {
			for (let attempt = 1; attempt <= maxRetries; attempt++) {
				const lastValue = input.value;
				
				// Ğ˜ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞµĞ¼ Ğ½Ğ°Ñ‚Ğ¸Ğ²Ğ½Ñ‹Ğ¹ setter Ğ´Ğ»Ñ Ğ¿Ñ€Ğ°Ğ²Ğ¸Ğ»ÑŒĞ½Ğ¾Ğ¹ Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‹ Ñ React
				// ĞŸĞ¾Ğ´Ğ´ĞµÑ€Ğ¶Ğ¸Ğ²Ğ°ĞµĞ¼ ĞºĞ°Ğº input, Ñ‚Ğ°Ğº Ğ¸ textarea
				const isTextarea = input.tagName.toLowerCase() === 'textarea';
				const prototype = isTextarea 
					? window.HTMLTextAreaElement.prototype 
					: window.HTMLInputElement.prototype;
				
				const nativeValueSetter = Object.getOwnPropertyDescriptor(prototype, 'value')?.set;
				
				if (nativeValueSetter) {
					nativeValueSetter.call(input, value);
				} else {
					input.value = value;
				}
				
				// Ğ Ğ°Ğ±Ğ¾Ñ‚Ğ°ĞµĞ¼ Ñ React Ñ‚Ñ€ĞµĞºĞµÑ€Ğ¾Ğ¼
				const tracker = input._valueTracker;
				if (tracker) {
					try {
						tracker.setValue(lastValue);
					} catch (e) {
						// Ğ˜Ğ³Ğ½Ğ¾Ñ€Ğ¸Ñ€ÑƒĞµĞ¼ Ğ¾ÑˆĞ¸Ğ±ĞºĞ¸ Ñ‚Ñ€ĞµĞºĞµÑ€Ğ°
					}
				}
				
				// Ğ”Ğ¸ÑĞ¿Ğ°Ñ‚Ñ‡Ğ¸Ğ¼ ÑĞ¾Ğ±Ñ‹Ñ‚Ğ¸Ñ Ñ bubbles: true Ğ´Ğ»Ñ React, Ğ½Ğ¾ Ñ Ñ„Ğ»Ğ°Ğ³Ğ¾Ğ¼ Ğ´Ğ»Ñ Ğ±Ğ»Ğ¾ĞºĞ¸Ñ€Ğ¾Ğ²ĞºĞ¸ workflow
				const inputEvent = new Event('input', { bubbles: true, cancelable: true });
				const changeEvent = new Event('change', { bubbles: true, cancelable: true });
				const blurEvent = new Event('blur', { bubbles: true, cancelable: true });
				
				// Ğ”Ğ¾Ğ±Ğ°Ğ²Ğ»ÑĞµĞ¼ Ñ„Ğ»Ğ°Ğ³ Ğ´Ğ»Ñ Ğ¸Ğ´ĞµĞ½Ñ‚Ğ¸Ñ„Ğ¸ĞºĞ°Ñ†Ğ¸Ğ¸ Ğ¿Ñ€Ğ¾Ğ³Ñ€Ğ°Ğ¼Ğ¼Ğ½Ñ‹Ñ… ÑĞ¾Ğ±Ñ‹Ñ‚Ğ¸Ğ¹
				if (silent) {
					inputEvent._programmatic = true;
					changeEvent._programmatic = true;
					blurEvent._programmatic = true;
				}
				
				input.dispatchEvent(inputEvent);
				input.dispatchEvent(changeEvent);
				input.dispatchEvent(blurEvent);
				
				await wait(150);
				
				// ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼, Ñ‡Ñ‚Ğ¾ Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ğµ ÑƒÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ¸Ğ»Ğ¾ÑÑŒ
				if (input.value === String(value)) {
					return true;
				}
				
				// Ğ•ÑĞ»Ğ¸ Ğ½Ğµ ÑƒÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ¸Ğ»Ğ¾ÑÑŒ Ğ¸ ÑÑ‚Ğ¾ Ğ½Ğµ Ğ¿Ğ¾ÑĞ»ĞµĞ´Ğ½ÑÑ Ğ¿Ğ¾Ğ¿Ñ‹Ñ‚ĞºĞ° - Ğ¶Ğ´ĞµĞ¼ Ğ¸ Ğ¿Ñ€Ğ¾Ğ±ÑƒĞµĞ¼ ÑĞ½Ğ¾Ğ²Ğ°
				if (attempt < maxRetries) {
					await wait(200);
				}
			}
			
