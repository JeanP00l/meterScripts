        async function waitForSelector(selector, timeout = 10000) {\r\n          const start = Date.now();\r\n          while (Date.now() - start < timeout) {\r\n            const el = document.querySelector(selector);\r\n            if (el) return el;\r\n            await wait(100);\r\n          }\r\n          return null;\r\n        }\r\n\r\n        // ===== Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ¿Ğ¾Ğ´ÑÑ‡ĞµÑ‚Ğ° ÑÑ‡ĞµÑ‚Ñ‡Ğ¸ĞºĞ¾Ğ² =====\r\n        async function countAllCounters() {\r\n          try {\r\n      // ĞŸĞµÑ€ĞµÑ…Ğ¾Ğ´ Ğ½Ğ° Ğ³Ğ»Ğ°Ğ²Ğ½ÑƒÑ ÑÑ‚Ñ€Ğ°Ğ½Ğ¸Ñ†Ñƒ\r\n      if (window.location.href !== 'https://meter.printecs.com/') {\r\n        window.location.href = 'https://meter.printecs.com/';\r\n        await wait(2000); // Ğ–Ğ´ĞµĞ¼ Ğ½Ğ°Ñ‡Ğ°Ğ»Ğ° Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ¸\r\n      }\r\n\r\n      // Ğ–Ğ´ĞµĞ¼ Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ¸ Ğ°Ğ´Ñ€ĞµÑĞ¾Ğ²\r\n      showToast('â³ ĞĞ¶Ğ¸Ğ´Ğ°Ğ½Ğ¸Ğµ Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ¸ Ğ°Ğ´Ñ€ĞµÑĞ¾Ğ²...', 3000, '#555');\r\n      const addressesContainer = await waitForSelector('div._tasksContainer_36r29_11', 30000);\r\n      \r\n      if (!addressesContainer) {\r\n        showToast('âŒ ĞĞµ ÑƒĞ´Ğ°Ğ»Ğ¾ÑÑŒ Ğ·Ğ°Ğ³Ñ€ÑƒĞ·Ğ¸Ñ‚ÑŒ ÑĞ¿Ğ¸ÑĞ¾Ğº Ğ°Ğ´Ñ€ĞµÑĞ¾Ğ²', 5000, '#a33');\r\n        return;\r\n      }\r\n\r\n      // ĞĞ°Ñ…Ğ¾Ğ´Ğ¸Ğ¼ Ğ²ÑĞµ Ğ°Ğ´Ñ€ĞµÑĞ°\r\n      // ĞĞ´Ñ€ĞµÑĞ° Ğ½Ğ°Ñ…Ğ¾Ğ´ÑÑ‚ÑÑ Ğ² div._taskItem_36r29_38 Ğ¸ Ğ¸Ğ¼ĞµÑÑ‚ span Ğ²Ğ½ÑƒÑ‚Ñ€Ğ¸ _taskTitle_36r29_45\r\n      await wait(1000); // Ğ”Ğ¾Ğ¿Ğ¾Ğ»Ğ½Ğ¸Ñ‚ĞµĞ»ÑŒĞ½Ğ°Ñ Ğ·Ğ°Ğ´ĞµÑ€Ğ¶ĞºĞ° Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ»Ğ½Ğ¾Ğ¹ Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ¸\r\n      \r\n      // Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ»ÑƒÑ‡ĞµĞ½Ğ¸Ñ ÑĞ¿Ğ¸ÑĞºĞ° Ğ°Ğ´Ñ€ĞµÑĞ¾Ğ²\r\n      function getAddressesList() {\r\n        const allAddressItems = document.querySelectorAll('div._taskItem_36r29_38');\r\n        const addresses = [];\r\n        \r\n        // Ğ¤Ğ¸Ğ»ÑŒÑ‚Ñ€ÑƒĞµĞ¼ Ğ°Ğ´Ñ€ĞµÑĞ°: Ñ‚Ğµ, Ñƒ ĞºĞ¾Ñ‚Ğ¾Ñ€Ñ‹Ñ… Ğ² _taskTitle_36r29_45 ĞµÑÑ‚ÑŒ span (ÑÑ‚Ğ¾ Ğ°Ğ´Ñ€ĞµÑĞ°, Ğ° Ğ½Ğµ ĞºĞ²Ğ°Ñ€Ñ‚Ğ¸Ñ€Ñ‹)\r\n        allAddressItems.forEach(item => {\r\n          const titleElement = item.querySelector('div._taskTitle_36r29_45');\r\n