        if (lowerTranscript.includes('ÑÑ‡ĞµÑ‚Ñ‡Ğ¸Ğº') || lowerTranscript.includes('ÑÑ‡Ñ‘Ñ‚Ñ‡Ğ¸Ğº')) {
          // Ğ˜Ğ·Ğ²Ğ»ĞµĞºĞ°ĞµĞ¼ Ñ‚ĞµĞºÑÑ‚ Ğ¿Ğ¾ÑĞ»Ğµ ÑĞ»Ğ¾Ğ²Ğ° "ÑÑ‡ĞµÑ‚Ñ‡Ğ¸Ğº" Ğ¸Ğ»Ğ¸ "ÑÑ‡Ñ‘Ñ‚Ñ‡Ğ¸Ğº"
          const counterMatch = lowerTranscript.match(/(?:ÑÑ‡ĞµÑ‚Ñ‡Ğ¸Ğº|ÑÑ‡Ñ‘Ñ‚Ñ‡Ğ¸Ğº)\s+(.+)/);
          if (!counterMatch) {
            showToast('âŒ ĞĞµ Ñ€Ğ°ÑĞ¿Ğ¾Ğ·Ğ½Ğ°Ğ½ Ñ‚Ğ¸Ğ¿ ÑÑ‡ĞµÑ‚Ñ‡Ğ¸ĞºĞ°', 3000, '#a33');
            return;
          }

          const counterPhrase = counterMatch[1].trim();
          
          // Ğ£Ğ±Ğ¸Ñ€Ğ°ĞµĞ¼ ÑĞ»Ğ¾Ğ²Ğ¾ "ÑĞ½ĞµÑ€Ğ³Ğ¾Ğ¼ĞµÑ€Ğ°" Ğ´Ğ»Ñ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸
          const phraseWithoutEnergy = counterPhrase.replace(/^ÑĞ½ĞµÑ€Ğ³Ğ¾Ğ¼ĞµÑ€Ğ°\s+/i, '').trim();
          
          // ĞĞ¾Ñ€Ğ¼Ğ°Ğ»Ğ¸Ğ·ÑƒĞµĞ¼ Ñ„Ñ€Ğ°Ğ·Ñƒ Ğ´Ğ»Ñ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ¸ (ÑƒĞ±Ğ¸Ñ€Ğ°ĞµĞ¼ Ğ²ÑĞµ Ğ¿Ñ€Ğ¾Ğ±ĞµĞ»Ñ‹, Ğ¿Ñ€Ğ¸Ğ²Ğ¾Ğ´Ğ¸Ğ¼ Ğº Ğ½Ğ¸Ğ¶Ğ½ĞµĞ¼Ñƒ Ñ€ĞµĞ³Ğ¸ÑÑ‚Ñ€Ñƒ)
          // Ğ¢Ğ°ĞºĞ¶Ğµ Ğ·Ğ°Ğ¼ĞµĞ½ÑĞµĞ¼ ĞºĞ¸Ñ€Ğ¸Ğ»Ğ»Ğ¸Ñ†Ñƒ Ğ½Ğ° Ğ»Ğ°Ñ‚Ğ¸Ğ½Ğ¸Ñ†Ñƒ Ğ´Ğ»Ñ ÑƒĞ½Ğ¸Ñ„Ğ¸ĞºĞ°Ñ†Ğ¸Ğ¸: Ñ€ -> r, Ñ -> s
          let normalizedPhrase = phraseWithoutEnergy.replace(/\s+/g, '').toLowerCase();
          normalizedPhrase = normalizedPhrase.replace(/Ñ€/g, 'r').replace(/Ñ/g, 's');
          
          // ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼, ÑĞ²Ğ»ÑĞµÑ‚ÑÑ Ğ»Ğ¸ ÑÑ‚Ğ¾ r5 (Ğ²ĞºĞ»ÑÑ‡Ğ°Ñ Ğ²Ğ°Ñ€Ğ¸Ğ°Ğ½Ñ‚Ñ‹ Ñ ĞºĞ¸Ñ€Ğ¸Ğ»Ğ»Ğ¸Ñ†ĞµĞ¹ Ñ€5)
          // ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ Ñ€Ğ°Ğ·Ğ»Ğ¸Ñ‡Ğ½Ñ‹Ğµ Ğ²Ğ°Ñ€Ğ¸Ğ°Ğ½Ñ‚Ñ‹: r5, ÑÑ€5, Ñ€5, rĞ¿ÑÑ‚ÑŒ, ÑÑ€Ğ¿ÑÑ‚ÑŒ, Ñ€Ğ¿ÑÑ‚ÑŒ
          const isR5Simple = normalizedPhrase === 'r5' || normalizedPhrase === 'ÑÑ€5' || 
                           normalizedPhrase === 'rĞ¿ÑÑ‚ÑŒ' || normalizedPhrase === 'ÑÑ€Ğ¿ÑÑ‚ÑŒ';
          
          // ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ Ñ‚Ğ¾Ñ‡Ğ½Ğ¾Ğµ ÑĞ¾Ğ²Ğ¿Ğ°Ğ´ĞµĞ½Ğ¸Ğµ Ñ Ğ¿Ñ€Ğ¾Ğ±ĞµĞ»Ğ°Ğ¼Ğ¸: r 5, ÑÑ€ 5, Ñ€ 5, r Ğ¿ÑÑ‚ÑŒ, ÑÑ€ Ğ¿ÑÑ‚ÑŒ, Ñ€ Ğ¿ÑÑ‚ÑŒ
          const normalizedForRegex = phraseWithoutEnergy.toLowerCase().replace(/Ñ€/g, 'r');
          const isR5WithSpace = /^(ÑÑ€|r)\s*5\s*$|^(ÑÑ€|r)\s*Ğ¿ÑÑ‚ÑŒ\s*$/i.test(normalizedForRegex);
          
          // ĞĞ±ÑŠĞµĞ´Ğ¸Ğ½ĞµĞ½Ğ½Ğ°Ñ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ĞºĞ°: ÑÑ‚Ğ¾ r5 (Ğ»ÑĞ±Ğ¾Ğ¹ Ğ²Ğ°Ñ€Ğ¸Ğ°Ğ½Ñ‚)
          const isR5Ambiguous = isR5Simple || isR5WithSpace;
          
          // Ğ•ÑĞ»Ğ¸ ÑÑ‚Ğ¾ r5 (Ğ»ÑĞ±Ğ¾Ğ¹ Ğ²Ğ°Ñ€Ğ¸Ğ°Ğ½Ñ‚) - Ğ¿Ğ¾ĞºĞ°Ğ·Ñ‹Ğ²Ğ°ĞµĞ¼ Ğ²Ñ‹Ğ±Ğ¾Ñ€
