	async function waitForSelector(sel, timeout = 5000) {
		const start = Date.now();
		while (Date.now() - start < timeout) {
			const el = document.querySelector(sel);
			if (el) return el;
			await wait(100);
		}
		throw new Error(`Ğ­Ğ»ĞµĞ¼ĞµĞ½Ñ‚ Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½: ${sel}`);
	}
	
	async function findButtonByText(text, fallbackSelectors = [], timeout = 5000) {
		const start = Date.now();
		while (Date.now() - start < timeout) {
			// Ğ˜Ñ‰ĞµĞ¼ Ğ¿Ğ¾ Ñ‚ĞµĞºÑÑ‚Ñƒ Ğ² label Ğ²Ğ½ÑƒÑ‚Ñ€Ğ¸ button
			const buttons = Array.from(document.querySelectorAll('button'));
			const found = buttons.find(btn => {
				const label = btn.querySelector('._label_1xwkq_26, div');
				if (label && label.textContent.trim().includes(text)) {
					return true;
				}
				// Ğ¢Ğ°ĞºĞ¶Ğµ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ Ğ¿Ñ€ÑĞ¼Ğ¾Ğ¹ textContent
				return btn.textContent.trim().includes(text);
			});
			if (found) return found;
			
			// ĞŸÑ€Ğ¾Ğ±ÑƒĞµĞ¼ fallback ÑĞµĞ»ĞµĞºÑ‚Ğ¾Ñ€Ñ‹
			for (const sel of fallbackSelectors) {
				const el = document.querySelector(sel);
				if (el) return el;
			}
			
			await wait(100);
		}
		throw new Error(`ĞšĞ½Ğ¾Ğ¿ĞºĞ° "${text}" Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½Ğ°`);
	}
	
