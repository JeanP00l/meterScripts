	async function waitForSelector(sel, timeout = 5000) {
		const start = Date.now();
		while (Date.now() - start < timeout) {
			const el = document.querySelector(sel);
			if (el) return el;
			await wait(100);
		}
		throw new Error(`Ğ­Ğ»ĞµĞ¼ĞµĞ½Ñ‚ Ğ½Ğµ Ğ½Ğ°Ğ¹Ğ´ĞµĞ½: ${sel}`);
	}
	
