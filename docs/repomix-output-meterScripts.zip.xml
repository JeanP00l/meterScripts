<file path="0-9-3/ÂŒÂ¥Â­Â¥Â¤Â¦Â¥Ã  Â®Â¡Ã©Â¥Â© Â¯Â Â­Â¥Â«Â¨ Â­Â Ã¡Ã¢Ã Â®Â¥Âª.txt">
// ĞœĞµĞ½ĞµĞ´Ğ¶ĞµÑ€ Ğ¾Ğ±Ñ‰ĞµĞ¹ Ğ¿Ğ°Ğ½ĞµĞ»Ğ¸ Ğ½Ğ°ÑÑ‚Ñ€Ğ¾ĞµĞº
(() => {
  'use strict';

  // ===== ĞĞ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸ =====
  const positionKey = 'unified_panel_position';
  let panel = null;
  let content = null;
  let collapsed = null;
  const sections = [];

  function initPanel() {
    if (panel) return;

    panel = document.createElement('div');
    panel.style.cssText = `
      position: fixed;
      top: 10px;
      right: 250px;
      background: rgba(255,255,255,0.95);
      color: #000;
      font-family: sans-serif;
      font-size: 13px;
      padding: 10px;
      border: 1px solid #ccc;
      border-radius: 10px;
      box-shadow: 0 2px 6px rgba(0,0,0,0.2);
      z-index: 999999;
      width: 280px;
      max-height: 90vh;
      overflow-y: auto;
      transition: all 0.3s ease;
    `;
    document.body.appendChild(panel);

    const savedPosition = JSON.parse(localStorage.getItem(positionKey) || '{}');
    if (savedPosition.left && savedPosition.top) {
      panel.style.left = savedPosition.left;
      panel.style.top = savedPosition.top;
      panel.style.right = '';
    } else {
      const rect = panel.getBoundingClientRect();
      panel.style.left = `${rect.left}px`;
      panel.style.right = '';
    }

    collapsed = document.createElement('div');
    collapsed.style.cssText = `
      display: flex;
      justify-content: space-between;
      align-items: center;
      cursor: pointer;
      font-weight: bold;
      padding: 5px 0;
    `;
    collapsed.innerHTML = `
      <span class="drag-handle" style="cursor: move; margin-right: 5px; font-size: 14px;">â†”â†•</span>
      ĞĞ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸ ÑĞºÑ€Ğ¸Ğ¿Ñ‚Ğ¾Ğ²
      <span style="font-size:14px; cursor:pointer;">âš™ï¸</span>
    `;
    panel.appendChild(collapsed);

    content = document.createElement('div');
    content.style.display = 'none';
    panel.appendChild(content);

    collapsed.querySelector('span:last-child').addEventListener('click', () => {
      content.style.display = content.style.display === 'none' ? 'block' : 'none';
    });

    let isDragging = false;
    let dragOffsetX = 0;
    let dragOffsetY = 0;
    const dragHandle = collapsed.querySelector('.drag-handle');

    dragHandle.addEventListener('mousedown', (e) => {
      isDragging = true;
      const rect = panel.getBoundingClientRect();
      
      // Ğ’Ñ‹Ñ‡Ğ¸ÑĞ»ÑĞµĞ¼ ÑĞ¼ĞµÑ‰ĞµĞ½Ğ¸Ğµ ĞºÑƒÑ€ÑĞ¾Ñ€Ğ° Ğ¾Ñ‚Ğ½Ğ¾ÑĞ¸Ñ‚ĞµĞ»ÑŒĞ½Ğ¾ Ğ»ĞµĞ²Ğ¾Ğ³Ğ¾ Ğ²ĞµÑ€Ñ…Ğ½ĞµĞ³Ğ¾ ÑƒĞ³Ğ»Ğ° Ğ¿Ğ°Ğ½ĞµĞ»Ğ¸
      dragOffsetX = e.clientX - rect.left;
      dragOffsetY = e.clientY - rect.top;
      
      // ĞŸÑ€ĞµĞ´Ğ¾Ñ‚Ğ²Ñ€Ğ°Ñ‰Ğ°ĞµĞ¼ Ğ²Ñ‹Ğ´ĞµĞ»ĞµĞ½Ğ¸Ğµ Ñ‚ĞµĞºÑÑ‚Ğ° Ğ¸ Ğ´Ñ€ÑƒĞ³Ğ¸Ğµ Ğ´ĞµĞ¹ÑÑ‚Ğ²Ğ¸Ñ
      e.preventDefault();
      e.stopPropagation();
      
      // Ğ”Ğ¾Ğ±Ğ°Ğ²Ğ»ÑĞµĞ¼ ÑÑ‚Ğ¸Ğ»Ğ¸ Ğ´Ğ»Ñ Ğ¿Ñ€ĞµĞ´Ğ¾Ñ‚Ğ²Ñ€Ğ°Ñ‰ĞµĞ½Ğ¸Ñ Ğ²Ñ‹Ğ´ĞµĞ»ĞµĞ½Ğ¸Ñ
      document.body.style.userSelect = 'none';
      document.body.style.cursor = 'move';
      panel.style.pointerEvents = 'none';
      
      // Ğ˜ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞµĞ¼ capture phase Ğ´Ğ»Ñ Ğ±Ğ¾Ğ»ĞµĞµ Ğ±Ñ‹ÑÑ‚Ñ€Ğ¾Ğ¹ Ğ¾Ğ±Ñ€Ğ°Ğ±Ğ¾Ñ‚ĞºĞ¸
      document.addEventListener('mousemove', onMouseMove, true);
      document.addEventListener('mouseup', onMouseUp, true);
    });

    function onMouseMove(e) {
      if (!isDragging) return;
      e.preventDefault();
      e.stopPropagation();
      
      // Ğ’Ñ‹Ñ‡Ğ¸ÑĞ»ÑĞµĞ¼ Ğ½Ğ¾Ğ²ÑƒÑ Ğ¿Ğ¾Ğ·Ğ¸Ñ†Ğ¸Ñ Ğ½Ğ°Ğ¿Ñ€ÑĞ¼ÑƒÑ Ğ¾Ñ‚ ĞºÑƒÑ€ÑĞ¾Ñ€Ğ° - Ğ¶ĞµÑÑ‚ĞºĞ¾Ğµ ÑĞ»ĞµĞ´Ğ¾Ğ²Ğ°Ğ½Ğ¸Ğµ
      let newLeft = e.clientX - dragOffsetX;
      let newTop = e.clientY - dragOffsetY;
      
      // ĞŸĞ¾Ğ»ÑƒÑ‡Ğ°ĞµĞ¼ Ñ€Ğ°Ğ·Ğ¼ĞµÑ€Ñ‹ Ğ¿Ğ°Ğ½ĞµĞ»Ğ¸
      const panelWidth = panel.offsetWidth;
      const panelHeight = panel.offsetHeight;
      
      // ĞĞ³Ñ€Ğ°Ğ½Ğ¸Ñ‡Ğ¸Ğ²Ğ°ĞµĞ¼ Ğ¿ĞµÑ€ĞµĞ¼ĞµÑ‰ĞµĞ½Ğ¸Ğµ Ğ³Ñ€Ğ°Ğ½Ğ¸Ñ†Ğ°Ğ¼Ğ¸ Ğ¾ĞºĞ½Ğ° - Ğ¿Ğ°Ğ½ĞµĞ»ÑŒ ÑƒĞ¿Ğ¸Ñ€Ğ°ĞµÑ‚ÑÑ ĞºÑ€Ğ°ÑĞ¼Ğ¸
      // Ğ›ĞµĞ²Ñ‹Ğ¹ ĞºÑ€Ğ°Ğ¹ Ğ½Ğµ Ğ¼Ğ¾Ğ¶ĞµÑ‚ Ğ±Ñ‹Ñ‚ÑŒ Ğ¼ĞµĞ½ÑŒÑˆĞµ 0
      if (newLeft < 0) newLeft = 0;
      // ĞŸÑ€Ğ°Ğ²Ñ‹Ğ¹ ĞºÑ€Ğ°Ğ¹ Ğ½Ğµ Ğ¼Ğ¾Ğ¶ĞµÑ‚ Ğ±Ñ‹Ñ‚ÑŒ Ğ±Ğ¾Ğ»ÑŒÑˆĞµ ÑˆĞ¸Ñ€Ğ¸Ğ½Ñ‹ Ğ¾ĞºĞ½Ğ°
      if (newLeft + panelWidth > window.innerWidth) newLeft = window.innerWidth - panelWidth;
      // Ğ’ĞµÑ€Ñ…Ğ½Ğ¸Ğ¹ ĞºÑ€Ğ°Ğ¹ Ğ½Ğµ Ğ¼Ğ¾Ğ¶ĞµÑ‚ Ğ±Ñ‹Ñ‚ÑŒ Ğ¼ĞµĞ½ÑŒÑˆĞµ 0
      if (newTop < 0) newTop = 0;
      // ĞĞ¸Ğ¶Ğ½Ğ¸Ğ¹ ĞºÑ€Ğ°Ğ¹ Ğ½Ğµ Ğ¼Ğ¾Ğ¶ĞµÑ‚ Ğ±Ñ‹Ñ‚ÑŒ Ğ±Ğ¾Ğ»ÑŒÑˆĞµ Ğ²Ñ‹ÑĞ¾Ñ‚Ñ‹ Ğ¾ĞºĞ½Ğ°
      if (newTop + panelHeight > window.innerHeight) newTop = window.innerHeight - panelHeight;
      
      // ĞŸÑ€Ğ¸Ğ¼ĞµĞ½ÑĞµĞ¼ Ğ¿Ğ¾Ğ·Ğ¸Ñ†Ğ¸Ñ Ğ½Ğ°Ğ¿Ñ€ÑĞ¼ÑƒÑ, Ğ±ĞµĞ· transform - Ğ¶ĞµÑÑ‚ĞºĞ¾Ğµ ÑĞ»ĞµĞ´Ğ¾Ğ²Ğ°Ğ½Ğ¸Ğµ Ğ±ĞµĞ· Ğ¸Ğ½ĞµÑ€Ñ†Ğ¸Ğ¸
      panel.style.left = `${newLeft}px`;
      panel.style.top = `${newTop}px`;
      panel.style.right = 'auto';
      panel.style.margin = '0';
      panel.style.transform = 'none';
    }

    function onMouseUp(e) {
      if (!isDragging) return;
      isDragging = false;
      
      // Ğ’Ğ¾ÑÑÑ‚Ğ°Ğ½Ğ°Ğ²Ğ»Ğ¸Ğ²Ğ°ĞµĞ¼ Ğ½Ğ¾Ñ€Ğ¼Ğ°Ğ»ÑŒĞ½Ğ¾Ğµ Ğ¿Ğ¾Ğ²ĞµĞ´ĞµĞ½Ğ¸Ğµ
      document.body.style.userSelect = '';
      document.body.style.cursor = '';
      panel.style.pointerEvents = '';
      
      document.removeEventListener('mousemove', onMouseMove, true);
      document.removeEventListener('mouseup', onMouseUp, true);
      
      // Ğ¡Ğ¾Ñ…Ñ€Ğ°Ğ½ÑĞµĞ¼ Ğ¿Ğ¾Ğ·Ğ¸Ñ†Ğ¸Ñ
      localStorage.setItem(positionKey, JSON.stringify({
        left: panel.style.left,
        top: panel.style.top
      }));
    }
  }

  function registerSection(id, title, htmlContent, callbacks) {
    initPanel();
    sections.push({ id, title, htmlContent, callbacks });
    renderSections();
  }

  function renderSections() {
    if (!content) return;

    let html = `
      <div style="display:flex; justify-content:space-between; align-items:center; margin-bottom:10px;">
        <span style="font-weight:bold;">ĞĞ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸ ÑĞºÑ€Ğ¸Ğ¿Ñ‚Ğ¾Ğ²</span>
        <span id="toggleCollapseUnified" style="cursor:pointer; font-size:14px;">ğŸ™ˆ</span>
      </div>
    `;

    sections.forEach((section, index) => {
      // Ğ’ÑĞµ ÑĞµĞºÑ†Ğ¸Ğ¸ ÑĞ²ĞµÑ€Ğ½ÑƒÑ‚Ñ‹ Ğ¿Ğ¾ ÑƒĞ¼Ğ¾Ğ»Ñ‡Ğ°Ğ½Ğ¸Ñ
      html += `
        <div class="section-${section.id}" style="margin-bottom:${index < sections.length - 1 ? '10px' : '0'}; border: 1px solid #ddd; border-radius: 5px; overflow: hidden;">
          <div class="section-header-${section.id}" style="background: #f5f5f5; padding: 8px; cursor: pointer; font-weight: bold; display: flex; justify-content: space-between; align-items: center;" onclick="window.meterScriptsPanelManager.toggleSection('${section.id}')">
            <span>${section.title}</span>
            <span class="section-arrow-${section.id}">â–¶</span>
          </div>
          <div class="section-content-${section.id}" style="padding: 10px; display: none;">
            ${section.htmlContent}
          </div>
        </div>
      `;
    });

    content.innerHTML = html;

    const toggleBtn = content.querySelector('#toggleCollapseUnified');
    if (toggleBtn) {
      toggleBtn.addEventListener('click', () => {
        content.style.display = 'none';
        collapsed.style.display = 'flex';
      });
    }

    // Ğ’Ñ‹Ğ·Ñ‹Ğ²Ğ°ĞµĞ¼ callbacks Ğ´Ğ»Ñ Ğ¸Ğ½Ğ¸Ñ†Ğ¸Ğ°Ğ»Ğ¸Ğ·Ğ°Ñ†Ğ¸Ğ¸ ÑĞ»ĞµĞ¼ĞµĞ½Ñ‚Ğ¾Ğ²
    sections.forEach(section => {
      if (section.callbacks && section.callbacks.onRender) {
        const sectionContainer = content.querySelector(`.section-content-${section.id}`);
        if (sectionContainer) {
          section.callbacks.onRender(sectionContainer);
        }
      }
    });
  }

  function toggleSection(id) {
    if (!content) return;
    const sectionContent = content.querySelector(`.section-content-${id}`);
    const arrow = content.querySelector(`.section-arrow-${id}`);
    if (sectionContent && arrow) {
      if (sectionContent.style.display === 'none') {
        sectionContent.style.display = 'block';
        arrow.textContent = 'â–¼';
      } else {
        sectionContent.style.display = 'none';
        arrow.textContent = 'â–¶';
      }
    }
  }

  // Ğ­ĞºÑĞ¿Ğ¾Ñ€Ñ‚Ğ¸Ñ€ÑƒĞµĞ¼ Ğ¼ĞµĞ½ĞµĞ´Ğ¶ĞµÑ€ Ğ² window
  window.meterScriptsPanelManager = {
    registerSection,
    toggleSection
  };

})();
